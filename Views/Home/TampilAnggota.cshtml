@{
    ViewData["Title"] = "Daftar Anggota";
}
<h2>Daftar Anggota</h2>
<table class="table table-striped">
    <tr>
        <th>Username</th>
        <th>Nama</th>
        <th>Alamat</th>
        <th>Email</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Username</td>
            <td>@item.Nama</td>
            <td>@item.Alamat</td>
            <td>@item.Email</td>
        </tr>
    }
</table>